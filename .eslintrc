{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "mocha": true
    },
    "rules": {
        "comma-dangle": [
            "error",
            "only-multiline"
        ],
        "no-console": 0,
        "linebreak-style": ["error", "windows"],
        "no-restricted-syntax": 0
    }
}